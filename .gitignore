	public static void Display()
	{
		for(int x = 1; x<=200; x+=10)
			System.out.println(x);
	}
	
	public static double Average(int X, int Y)
	{
		double Avg = (X+Y)/2.0;
		return Avg;
	}
	
	public static void SelectionSort(int List[],int count)
	{
		for(int start = 0; start< count; start++)
		{
			int min = start;
			for(int x = start+1; x < count; x++)
			{
				if (min>x);
				min = x;
			}
			Swop(start,min,List);
		}
	}
	
	public static void Swop(int a, int b, int List [])
	{
		int temp = List[a];
		List[a]=List[b];
		List [b]=temp;
	}
	
	

}

